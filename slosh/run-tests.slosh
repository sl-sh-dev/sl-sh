#!/usr/bin/env slosh

(let (globals (get-globals-sorted)
      globals-len (len globals)
      i 0)
    (while (< i globals-len)
        (prn "XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX")
        (prn globals.~i)
        (prn (get-prop globals.~i :doc-string))
        (prn (doc-map globals.~i))
        (inc! i)))
